    <!-- http://purl.obolibrary.org/obo/GO_0007610 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/GO_0007610">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000060"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The internally coordinated responses (actions or inactions) of animals (individuals or groups) to internal or external stimuli, via a mechanism that involves nervous system activity.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/go.owl"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.obolibrary.org/obo/pain/mfoem_import.owl"/>
