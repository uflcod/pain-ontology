        <rdfs:label xml:lang="en">canonical pain</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PAIN_0000002 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PAIN_0000002">
