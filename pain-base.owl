    <!-- http://purl.obolibrary.org/obo/PAIN_0000040 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PAIN_0000040">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000203"/>
