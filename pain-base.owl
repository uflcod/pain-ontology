                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/PATO_0001863"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>A feeling of pain that persists or recurs over an extended period of time in which each incidence of pain is attributed to the same underlying condition.</obo:IAO_0000115>
        <dc:contributor>https://orcid.org/0000-0001-9625-1899</dc:contributor>
        <oboInOwl:creation_date>2022-11-24T06:02:26Z</oboInOwl:creation_date>
        <rdfs:comment>Chronic pain persists or recurs for longer than three months and may persist despite successful management of the condition that initially caused it, or because the underlying medical condition cannot be treated successfully.</rdfs:comment>
        <rdfs:label xml:lang="en">chronic pain</rdfs:label>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://purl.obolibrary.org/obo/PAIN_0000040"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
        <owl:annotatedTarget>Chronic pain persists or recurs for longer than three months and may persist despite successful management of the condition that initially caused it, or because the underlying medical condition cannot be treated successfully.</owl:annotatedTarget>
        <dc:source>https://www.iasp-pain.org/advocacy/definitions-of-chronic-pain-syndromes/</dc:source>
    </owl:Axiom>
    


    <!-- http://purl.obolibrary.org/obo/PAIN_0000042 -->

